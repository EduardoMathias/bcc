program SomaPar;

var it,n,seq,soma:longint;

begin
    it:=1;
    read(n);
    if(n mod 2 <> 0) then
    writeln('Soma: impar')
    else
    begin
     while(it <= n) do
        begin
          read(seq);
          if(seq mod 2 = 0) then
            soma:= soma + seq;
            it:= it +1;
        end;
        writeln('Soma: ',soma);
    end;
end.
